##########################   Clocks   #####################################

# Differential 50MHz system clock
NET sys_clk_p           LOC = G11 | IOSTANDARD = LVDS_25;
NET sys_clk_n           LOC = F10 | IOSTANDARD = LVDS_25;

NET "clk_pll" TNM_NET = "clk_master";
TIMEGRP "tgrp_clk_master" = "clk_master";
TIMESPEC "TS_clk_master" = PERIOD "trgp_clk_master" 20 ns HIGH 50%;

##########################   LEDs   #####################################

NET leds<0>                 LOC = M16 | IOSTANDARD = LVCMOS25;
NET leds<1>                 LOC = K15 | IOSTANDARD = LVCMOS25;

##########################   GTX Transceivers   ###########################

# Constraints are mostly in-firmware

# External reference
#NET gt_ext_clk_p LOC = F6 | IOSTANDARD = LVDS;
#NET gt_ext_clk_n LOC = F5 | IOSTANDARD = LVDS;

# SIT9121 one-time programmable oscillator (125MHz)
#NET gt_ext_clk_p LOC = D6 | IOSTANDARD = LVDS;
#NET gt_ext_clk_n LOC = D5 | IOSTANDARD = LVDS;

# SI57X_A clock
NET gt_ext_clk_p LOC = H6 | IOSTANDARD = LVDS;
NET gt_ext_clk_n LOC = H5 | IOSTANDARD = LVDS;

# SI57X_B clock
#NET gt_ext_clk_p LOC = K6 | IOSTANDARD = LVDS;
#NET gt_ext_clk_n LOC = K5 | IOSTANDARD = LVDS;

##########################   Communications   #######################################

NET kintex_done LOC = F14 | IOSTANDARD = LVCMOS25 | DRIVE = 4; #3p

NET kintex_data_out_p LOC = J11 | IOSTANDARD = LVDS_25 | DRIVE = 4; # 1N
NET kintex_data_out_n LOC = J10 | IOSTANDARD = LVDS_25 | DRIVE = 4; # 1P

NET kintex_data_in_p LOC = J13 | IOSTANDARD = LVDS_25; # 2P
NET kintex_data_in_n LOC = H13 | IOSTANDARD = LVDS_25; # 2N

